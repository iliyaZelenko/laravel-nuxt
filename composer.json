{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-gd": "*",
        "barryvdh/laravel-cors": "^0.11.0",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.4",
        "giggsey/libphonenumber-for-php": "^8.9",
        "intervention/image": "^2.4",
        "itsgoingd/clockwork": "^2.2",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "pallares/laravel-nuxt": "^1.0",
        "pragmarx/countries-laravel": "^0.5.2",
        "propaganistas/laravel-phone": "^4.0",
        "socialiteproviders/instagram": "^3.0",
        "socialiteproviders/reddit": "^3.0",
        "socialiteproviders/twitter": "^3.0",
        "socialiteproviders/vkontakte": "^4.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "cp .env.example .env"
            "php artisan clear-compiled",
	        "php artisan key:generate",
	        "chmod -R 777 storage",
	        "php artisan passport:keys"
	    ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
